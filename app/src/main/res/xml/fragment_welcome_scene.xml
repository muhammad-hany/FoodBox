<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:motionInterpolator="linear"
        motion:autoTransition="jumpToEnd">
        <KeyFrameSet>
            <KeyAttribute
                android:scaleY="0.5"
                motion:framePosition="50"
                motion:motionTarget="@+id/swipe_up_icon" />
        </KeyFrameSet>
        <OnSwipe
            motion:touchAnchorId="@+id/editTextTextPersonName2"
            motion:touchAnchorSide="top"
            motion:dragDirection="dragUp"
            />


    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/backgroundImage"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.0">
            <CustomAttribute
                motion:attributeName="cornerRadius"
                motion:customDimension="0dp" />
        </Constraint>
        <Constraint
            android:id="@+id/icon_center_Image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.501"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/icon_center_Image" />
        <Constraint
            android:id="@+id/editTextTextPersonName2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/swipe_up_icon"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginBottom="8dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
            motion:layout_editor_absoluteX="68dp" />
        <Constraint
            android:id="@+id/imageView3"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:id="@+id/search_image"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintBottom_toBottomOf="@id/editTextTextPersonName2"
            motion:layout_constraintEnd_toEndOf="@+id/editTextTextPersonName2"
            motion:layout_constraintTop_toTopOf="@id/editTextTextPersonName2" />
        <Constraint
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            motion:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName2"
            motion:layout_constraintStart_toStartOf="@+id/editTextTextPersonName2"
            motion:layout_constraintTop_toTopOf="@+id/editTextTextPersonName2" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/icon_center_Image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/textView"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/backgroundImage"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.501"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/editTextTextPersonName2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintBottom_toBottomOf="@+id/backgroundImage"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="@+id/backgroundImage"
            motion:layout_constraintVertical_bias="0.0" />
        <Constraint

            android:id="@+id/backgroundImage"
            android:layout_width="0dp"
            android:layout_height="80dp"
            android:layout_marginBottom="0dp"
            motion:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName2"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
            <CustomAttribute
                motion:attributeName="cornerRadius"
                motion:customDimension="20dp" />
        </Constraint>
        <Constraint
            android:id="@+id/swipe_up_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2" />
        <Constraint
            android:id="@+id/search_image"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            motion:layout_constraintBottom_toBottomOf="@id/editTextTextPersonName2"
            motion:layout_constraintStart_toStartOf="@+id/editTextTextPersonName2"
            motion:layout_constraintTop_toTopOf="@id/editTextTextPersonName2" />
        <Constraint
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            motion:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName2"
            motion:layout_constraintStart_toEndOf="@+id/search_image"
            motion:layout_constraintTop_toTopOf="@+id/editTextTextPersonName2" />
    </ConstraintSet>
</MotionScene>